@model IEnumerable<Student>
@{
    ViewData["Title"] = "Students";
}

<div class="row">
    <div class="col-md-4">
        <table class="table">
            <tr>
                <th>@Html.DisplayNameFor(x => x.GetEnumerator().Current.StudentID)</th>
                <th>@Html.DisplayNameFor(x => x.GetEnumerator().Current.LastName)</th>
                <th>@Html.DisplayNameFor(x => x.GetEnumerator().Current.FirstMidName)</th>
                <th>@Html.DisplayNameFor(x => x.GetEnumerator().Current.EnrollmentDate)</th>
                <th></th>
            </tr>

            @foreach (var student in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(x => student.StudentID)</td>
                    <td>@Html.DisplayFor(x => student.LastName)</td>
                    <td>@Html.DisplayFor(x => student.FirstMidName)</td>
                    <td>@Html.DisplayFor(x => student.EnrollmentDate)</td>
                </tr>
            }

        </table>
    </div>
</div>
